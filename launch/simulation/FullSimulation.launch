<launch>
	<!-- Start Gazebo simulation -->
	<include file="$(find lunabotics2020)/launch/simulation/StartGazebo.launch"/>

	<!-- Spawn SLRS -->
	<include file="$(find lunabotics2020)/launch/simulation/SpawnSLRS.launch"/>

	<!-- Spawn Dug robot model -->
	<include file="$(find lunabotics2020)/launch/simulation/SpawnDug.launch"/>

	<!-- Publishes transforms based on robot description -->
	<node name="robot_state_publisher" 
		pkg="robot_state_publisher" 
		type="robot_state_publisher" 
		respawn="false" 
		output="screen">
	</node>
</launch>